{
    "name": "corerely/js-translation-bundle",
    "description": "Expose translations as JSON on Frontend",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrii Oprysko",
            "email": "anjey.link@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Corerely\\JsTranslationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Corerely\\JsTranslationBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "symfony/http-foundation": "^6.4 || ^7.0",
        "symfony/translation": "^6.4 || ^7.0"
    },
    "require-dev": {
        "slevomat/coding-standard": "^7.0",
        "phpunit/phpunit": "^10.0",
        "symfony/framework-bundle": "^6.4 || ^7.0",
        "symfony/browser-kit": "^6.4 || ^7.0",
        "symfony/yaml": "^6.4 || ^7.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
